@using Vjezba_18032024.Models.ViewModels
@model List<PolaznikViewModel>

@{
    ViewData["Title"] = "Home Page";
}

<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Ime</th>
            <th scope="col">Prezime</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th scope="row">item.Id</th>
                <td>item.Ime</td>
                <td>item.Prezime</td>
            </tr>
        }
        
    </tbody>
</table>